{
  "name": "task-spider",
  "type": "module",
  "version": "0.3.1",
  "packageManager": "pnpm@7.27.1",
  "description": "a very light nodejs spider framework",
  "author": "Merieth <wanglu@mereith.com>",
  "license": "MIT",
  "funding": "https://github.com/mereithhh/node-spider",
  "homepage": "https://github.com/mereithhh/node-spider#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mereithhh/node-spider.git"
  },
  "bugs": "https://github.com/mereithhh/node-spider/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno example/novel.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cheerio": "1.0.0-rc.12",
    "iconv-lite": "^0.6.3",
    "mongodb": "^5.6.0",
    "prom-client": "^14.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "@antfu/ni": "^0.21.3",
    "@antfu/utils": "^0.7.4",
    "@types/node": "^18.16.16",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.6.0",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9",
    "vitest": "^0.31.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
